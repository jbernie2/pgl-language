public class token
{
	String type = null;
	String text = "";
	
	public token()
	{
		//empty constructor
	}
	public void setType(String x)
	{type = x;}
	public void setText(String x)
	{text = x;}
	public void setText(char x)
	{text += x;}
	
	public String getType()
	{return type;}
	public String getText()
	{return text;}
	
}
